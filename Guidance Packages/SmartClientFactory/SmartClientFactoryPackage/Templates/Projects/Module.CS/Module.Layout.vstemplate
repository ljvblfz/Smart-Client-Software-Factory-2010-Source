<VSTemplate Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
	<TemplateData>
		<Name>Add Module Layout (C#)</Name>
		<Description>Creates a layout module the CAB Module</Description>
		<ProjectType>CSharp</ProjectType>
		<ProjectSubType>
		</ProjectSubType>
		<SortOrder>1000</SortOrder>
		<CreateNewFolder>true</CreateNewFolder>
		<DefaultName>CAB Shell</DefaultName>
		<ProvideDefaultName>true</ProvideDefaultName>
		<LocationField>Enabled</LocationField>
		<EnableLocationBrowseButton>true</EnableLocationBrowseButton>
		<Icon>__TemplateIcon.ico</Icon>
    <RequiredFrameworkVersion>3.5</RequiredFrameworkVersion>
	</TemplateData>
	<TemplateContent>
		<Project TargetFileName="Module.Layout.csproj" File="Module.Layout.csproj.template" ReplaceParameters="true">
			<Folder Name="Properties" TargetFolderName="Properties">
				<ProjectItem ReplaceParameters="true" TargetFileName="AssemblyInfo.cs">AssemblyInfo.cs.template</ProjectItem>
			</Folder>
			<Folder Name="Constants" TargetFolderName="Constants">
				<ProjectItem ReplaceParameters="true" TargetFileName="CommandNames.cs">CommandNames.cs.template</ProjectItem>
				<ProjectItem ReplaceParameters="true" TargetFileName="EventTopicNames.cs">EventTopicNames.cs.template</ProjectItem>
				<ProjectItem ReplaceParameters="true" TargetFileName="UIExtensionSiteNames.cs">UIExtensionSiteNames.cs.template</ProjectItem>
				<ProjectItem ReplaceParameters="true" TargetFileName="WorkspaceNames.cs">WorkspaceNames.cs.template</ProjectItem>
			</Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="ILayoutView.cs">ILayoutView.cs.template</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="LayoutView.cs">LayoutView.cs.template</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="LayoutView.GeneratedCode.cs">LayoutView.GeneratedCode.cs.template</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="LayoutView.Designer.cs">LayoutView.Designer.cs.template</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="LayoutViewPresenter.cs">LayoutViewPresenter.cs.template</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="LayoutViewPresenter.GeneratedCode.cs">LayoutViewPresenter.GeneratedCode.cs.template</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="LayoutView.resx">LayoutView.resx</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="Module.cs">Module.Layout.cs.template</ProjectItem>
			<Folder Name="Services" TargetFolderName="Services" />
		</Project>
	</TemplateContent>
	<WizardExtension>
		<Assembly>Microsoft.Practices.RecipeFramework.VisualStudio, Version=2.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35</Assembly>
		<FullClassName>Microsoft.Practices.RecipeFramework.VisualStudio.Templates.UnfoldTemplate</FullClassName>
	</WizardExtension>
	<WizardData>
		<Template xmlns="http://schemas.microsoft.com/pag/gax-template" SchemaVersion="1.0">
		</Template>
	</WizardData>
</VSTemplate>